<h1>New room</h1>

<%= form_for @room, url: {action: 'create'} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :price %>
  <%= f.text_field :price %><br>
  <%= f.label :bedrooms %>
  <%= f.text_field :bedrooms %><br>
  <%= f.hidden_field(:user_id, :value => "#{current_user.id}") %>
  <%= f.submit 'create' %>
<% end %>
